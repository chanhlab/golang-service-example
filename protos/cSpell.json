// cSpell Settings
{
  // Version of the setting file.  Always 0.1
  "version": "0.1",
  // language - current active spelling language
  "language": "en-US",
  // words - list of words to be always considered correct
  "words": [
    "downlevel",
    "ESNEXT",
    "grpc",
    "gzipped",
    "Interop",
    "languageclient",
    "lerna",
    "nospace",
    "proto",
    "protobuf",
    "protoc",
    "quotemark",
    "RPCs",
    "sourcemap",
    "streetsidesoftware",
    "submodule",
    "tslib",
    "typechecking",
    "webview"
  ],
  // flagWords - list of words to be always considered incorrect
  // This is useful for offensive words and common spelling errors.
  // For example "hte" should be "the"
  "flagWords": [
    "hte"
  ],
  "ignorePaths": [
    "node_modules",
    "coverage",
    "temp"
  ],
  "languageSettings": [{
    "languageId": "*",
    "dictionaries": []
  }]
}
